{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "dnd-core": {
      "projectType": "library",
      "root": "projects/dnd-core",
      "sourceRoot": "projects/dnd-core/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "projects/dnd-core/tsconfig.lib.json",
            "project": "projects/dnd-core/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/dnd-core/tsconfig.lib.prod.json"
            }
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/dnd-core/src/test.ts",
            "tsConfig": "projects/dnd-core/tsconfig.spec.json",
            "karmaConfig": "projects/dnd-core/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/dnd-core/tsconfig.lib.json",
              "projects/dnd-core/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "dnd-tree": {
      "projectType": "library",
      "root": "projects/dnd-tree",
      "sourceRoot": "projects/dnd-tree/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "projects/dnd-tree/tsconfig.lib.json",
            "project": "projects/dnd-tree/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/dnd-tree/tsconfig.lib.prod.json"
            }
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/dnd-tree/src/test.ts",
            "tsConfig": "projects/dnd-tree/tsconfig.spec.json",
            "karmaConfig": "projects/dnd-tree/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/dnd-tree/tsconfig.lib.json",
              "projects/dnd-tree/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    }
  },
  "cli": {
    "analytics": "a8fa3a3d-e1d7-4c80-8db6-57b422e656f2"
  },
  "defaultProject": "dnd-core"
}
